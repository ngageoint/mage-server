  "scripts": {
    "start": "ng serve",
    "build": "npm-run-all build:core-lib build:app",
    "build:app": "ng build --configuration production=true",
    "build:core-lib": "ng build --project=core-lib",
    "ng": "ng",
    "test": "ng test --code-coverage=false --project=app",
    "test:core-lib": "ng test --project=core-lib",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless --project=app",
    "test-headless:core-lib": "ng test --watch=false --browsers=ChromeHeadless --project=core-lib",
    "ci:test": "npm-run-all test-headless:core-lib test-headless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pack:app": "cd dist && cd app && npm pack",
    "pack:core-lib": "cd dist && cd core-lib && npm pack",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },

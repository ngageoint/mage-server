{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["node"]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts",
    "admin/src/main.ts",
    "admin/src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts",
    "admin/src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
    "admin/src/**/*.ts",
    "admin/src/test.ts",
    "admin/src/**/*.spec.ts"
  ]
}

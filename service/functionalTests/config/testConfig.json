{
  "api": {
    "name": "MAGE (Mobile Awareness GEOINT Environment)",
    "version": {
      "major": 6,
      "minor": 1,
      "micro": 0
    },
    "provision": {
      "strategy": "uid"
    },
    "locationServices": true,
    "apk": {
      "version": "4.0.0",
      "supportedVersions": [
        "4.0",
        "5.1"
      ]
    }
  },
  "server": {
    "type": "PDC",
    "userBaseDirectory": "/var/lib/mage/users",
    "iconBaseDirectory": "/var/lib/mage/icons",
    "token": {
      "expiration": 28800
    },
    "mongodb": {
      "host": "localhost",
      "port": 27017,
      "db": "magedb",
      "minPoolSize": 5,
      "maxPoolSize": 5
    },
    "locationServices": {
      "enabled": true,
      "userCollectionLocationLimit": 100
    },
    "attachment": {
      "baseDirectory": "/var/lib/mage/attachments"
    }
  }
}

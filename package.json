    "cfenv": "1.0.3",
    "cookie-session": "1.3.2",
    "dbf": "0.1.0",
    "express": "4.16.3",
    "file-type": "7.6.0",
    "fs-extra": "4.0.3",
    "geojson-validation": "0.2.1",
    "geojson-vt": "3.1.3",
    "gm": "1.23.1",
    "google-auth-library": "0.11.0",
    "jose": "0.3.2",
    "json2csv": "4.5.1",
    "jszip": "2.5.0",
    "moment": "2.22.2",
    "mongodb-migrations": "0.8.5",
    "mongoose": "4.13.14",
    "mongoose-beautiful-unique-validation": "7.1.1",
    "multer": "^1.4.2",
    "openapi-enforcer": "^1.8.0",
    "openapi-enforcer-middleware": "^1.2.1",
    "openid-client": "2.2.1",
    "passport": "0.4.0",
    "passport-anonymous": "1.0.1",
    "passport-geoaxis-oauth20": "0.1.1",
    "passport-google-oauth": "2.0.0",
    "passport-http-bearer": "1.0.1",
    "passport-ldapauth": "2.1.3",
    "passport-local": "1.0.0",
    "pem-jwk": "1.5.1",
    "pem2jwk": "1.0.1",
    "pug": "2.0.4",
    "shp-write": "https://github.com/newmanw/shp-write.git#fix-polygon-export",
    "terraformer": "1.0.4",
    "terraformer-arcgis-parser": "1.0.4",
    "vt-pbf": "3.1.1",
    "walk": "2.3.4",
    "winston": "1.0.1",
    "wkx": "0.4.1",
    "wms-capabilities": "^0.4.0",
    "xmldom": "0.1.22",
    "xpath": "0.0.23"
  },
  "devDependencies": {
    "@types/async": "^3.0.3",
    "@types/chai": "^4.2.5",
    "@types/fs-extra": "^8.0.1",
    "@types/geojson": "^7946.0.7",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^4.7.51",
    "@types/supertest": "^2.0.8",
    "ajv": "^6.10.2",
    "assert": "2.0.0",
    "chai": "4.2.0",
    "coveralls": "3.0.4",
    "cpy-cli": "^2.0.0",
    "mocha": "6.2.1",
    "mock-fs": "4.10.1",
    "mongodb-memory-server": "^6.2.1",
    "nyc": "^14.1.1",
    "proxyquire": "2.1.0",
    "sinon": "7.5.0",
    "sinon-chai": "3.3.0",
    "sinon-mongoose": "2.3.0",
    "superagent": "5.0.6",
    "supertest": "4.0.2",
    "ts-mockito": "^2.5.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "yaml": "^1.7.1"
  },
  "nyc": {
    "temp-dir": "./.test_run",
    "report-dir": "./coverage",
    "reporter": [
      "lcov"
    ]
  },
  "scripts": {
    "build": "npm run build-ts && npm run build-webapp",
    "build-ts": "tsc && cpy --parents 'docs/**/*' dist/",
    "build-webapp": "npm --prefix public install && npm --prefix public run build",
    "run": "node dist/app.js",
    "run-debug": "ts-node app.js",
    "report": "nyc report",
    "test": "mocha",
    "coverage": "nyc npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "create-migration": "mm create --config=mm-config.js",
    "prestart": "sh bin/prestart.sh",
    "poststop": "sh bin/poststop.sh",
    "migrate": "node -e \"require('./migrate').runDatabaseMigrations().then(() => process.exit(0), err => {throw err})\"",
    "start": "node dist/app.js"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "4.0.0"
    }
  },
  "bundleDependencies": [],
  "private": true,
  "main": "app.js"
}

    "MAGE"
  ],
  "author": {
    "name": "NGA"
  },
  "contributors": [
    {
      "name": "Billy Newman",
      "email": "newmanw10@gmail.com"
    }
  ],
  "homepage": "https://www1.nga.mil",
  "engines": {
    "node": ">= 8.x <= 10.x",
    "npm": ">= 5.x"
  },
  "dependencies": {
    "@mapbox/togeojson": "0.16.0",
    "@ngageoint/geopackage": "1.1.7",
    "@turf/centroid": "~6.0.0",
    "@turf/kinks": "~6.0.0",
    "adm-zip": "0.4.9",
    "archiver": "1.3.0",
